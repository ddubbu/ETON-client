] : e.target.value\n    })\n  }\n\n  async function clickAddHandler(e, target='progress', id){\n    // TODO ğŸ˜ ì„œë²„ì—ì„œ ìƒˆë¡œ ìƒì„±í•œ ìƒˆë¡œìš´ id ë¨¼ì € ì£¼ì‹œê³ \n    const new_prg_id = '5'\n    \n    if(target === 'progress') {\n      await setProgresses({ \n        ...progresses, \n        [new_prg_id]: { // here\n          id : new_prg_id, // here\n          title : input.title,\n          task_priority : '', \n        }})\n      await setBoard({ ... board, prg_priority: board['prg_priority'] + `,${new_prg_id}` }); // here\n    } \n  }\n \n  // drag-n-drop\n  document.addEventListener('mousemove', drag_n_drop.handleMouseMove);\n\n  /* (ë) drag-drop */\n\n  return (\n    <div id=\"main-content\">\n      <section id=\"sub-nav-bar\">\n        <input className=\"btn-sub-nav-bar board_title\" value={board.title} onChange={(e)=>{eventHandler.titleModifyHandler(e, store, 'board')}}></input>\n        <span className=\"btn-sub-nav-bar divider\"></span>\n        <button name='member' className=\"btn-sub-nav-bar member\" onClick={(e)=>eventHandler.toggleModal(e, store)}>member</button>\n        \n        <button className=\"btn-sub-nav-bar invite\">invite</button>\n      </section>\n      {/* ëª¨ë‹¬ì€ position:absolute ì´ë¯€ë¡œ í•œêº¼ë²ˆì— ì •ì˜í•˜ì, ëˆ„ë¥´ë©´ í™œì„±í™”ë˜ë„ë¡ */}\n      { modals.member ? <MemberDorpDown members={members} />  : '' }\n      { modals.progress ? <PrgMenuDropDown store={store} /> : '' }\n      { modals.task ? <TaskMenuDropDown store={store} /> : '' }\n      { modals.task_edit ? <TaskInfoEdit store={store} /> : '' }\n\n      <section id=\"progress-wrapper\">\n        {\n          sortObject(progresses, board.prg_priority).map((progress, idx)=>{\n            return (\n              <>\n                <article className={`prg-dropzone prg-dropzone-${idx}`}></article>\n                <ProgressList key={idx}\n                  ids={{board_id: board.id, progress_id: progress.id}}\n                  store={store}\n                />\n              </>\n            )\n          })\n        }\n        <article className={`prg-dropzone prg-dropzone-${board.prg_priority.split(',').length}`}></article>\n        {/* ëˆ„ë¥´ê¸°ì „ê¹Œì§€ ìˆ¨ì–´ ìˆìŒ */}\n        <article className='progress form-add-progress'>\n          <input \n            name='title'\n            className='form-add-progress-input' \n            placeholder='Enter progress title...'\n            onChange={inputChangeHandler}\n          ></input>\n          <button className='form-add-progress-btn-add' onClick={clickAddHandler}>Add progress</button>\n          <button \n            className='form-add-progress-btn-cancle'\n            onClick={eventHandler.cancleAddInfo}\n          >X</button>\n        </article>\n        <button \n          className=\"btn-add-progress\" \n          onClick={e=>{eventHandler.clickAddSomething(e, 'progress')}}\n        > + Add another progress \n          </button>\n      </section>\n    </div>\n  )\n}\n\n\n","/home/ddubbu/git/ETON/1. ETON-client/src/components/board/ProgressList.js",["135","136","137","138","139","140","141"],"/home/ddubbu/git/ETON/1. ETON-client/src/helper/drag-n-drop.js",["142","143","144","145","146","147","148","149","150"],"import eventHandler from '../helper/eventHandler.js';\n\nexport default {\n  handleMouseDown : (e) => { // í´ë¦­í•œ ìš”ì†Œ ê°ì§€, .hold ì´ë¦„ ë¶™ì—¬ì£¼ê¸°\n    const $el = e.target;\n    const classList = $el.classList;\n\n    if( !classList.contains('progress') && !classList.contains('task') \n        || classList.contains('form-add-task')) { // Create ë¥¼ ìœ„í•œ ê³µê°„ ì›€ì§ì´ì§€ ì•Šë„ë¡ \n      return console.log('progress, task ë‘˜ë‹¤ ì•„ë‹˜')\n    }\n    if( !classList.contains('hold') ){\n      // target í´ë¦­í–ˆì„ ë•Œ, ë§ˆìš°ìŠ¤ ì»¤ì„œì˜ XYì¢Œí‘œ\n      const mouseX = e.clientX;\n      const mouseY = e.clientY;\n      \n      // ì„ íƒí•œ ìš”ì†Œì˜ XYì¢Œí‘œ (ì™¼ìª½ ìƒë‹¨ ëª¨ì„œë¦¬ ê¸°ì¤€)\n      const pos = $el.getBoundingClientRect();\n      const x = pos.x;\n      const y = pos.y;\n\n      // ì„ íƒí•œ ìš”ì†Œ ì•ˆì— ìˆëŠ” ë§ˆìš°ìŠ¤ ì»¤ì„œì˜ XYì¢Œí‘œ\n      const gapX = mouseX - x;\n      const gapY = mouseY - y;\n      \n      $el.setAttribute(\"gap-x\", gapX);\n      $el.setAttribute(\"gap-y\", gapY);\n      classList.add(\"hold\");\n    }\n  },\n  handleMouseMove : function(e){ // .hold ìš”ì†Œ > ì›€ì§ì„ > ìœ„ì¹˜ ë°˜ì˜í•˜ê¸°\n    const $el = document.querySelector(\".hold\");\n\n    if( $el ){\n      // ì›€ì§ì´ëŠ” ë§ˆìš°ìŠ¤ ì»¤ì„œì˜ XYì¢Œí‘œ : ìƒˆë¡œìš´ ìœ„ì¹˜\n      const mouseX = e.clientX;\n      const mouseY = e.clientY;\n            \n      // ì„ íƒí•œ ê³µ ì•ˆì— ìˆëŠ” ë§ˆìš°ìŠ¤ ì»¤ì„œì˜ XYì¢Œí‘œ\n      const gapX = $el.getAttribute(\"gap-x\");\n      const gapY = $el.getAttribute(\"gap-y\");\n      \n      // ë§ˆìš°ìŠ¤ ì»¤ì„œì˜ ìœ„ì¹˜ì— ë”°ë¥¸ ê³µì˜ XYì¢Œí‘œ\n      const x = mouseX - gapX;\n      const y = mouseY - gapY;\n      \n      // ê³µì˜ ìœ„ì¹˜ë¥¼ ë³€ê²½\n      $el.style.left = x +\"px\";\n      $el.style.top = y +\"px\";\n\n      //! progress ê°€ ì›€ì§ì¼ ë•ŒëŠ” ì„¸ë¡œë¡œë§Œ\n      if($el.classList.contains('progress')){\n        // $dropzone ìœ„ì¹˜ ê°ì§€\n        const $dropzones = document.querySelectorAll('.prg-dropzone');\n        $dropzones.forEach($dropzone=>{\n          const x = $dropzone.getBoundingClientRect().x;\n          if( mouseX > x && mouseX < x + 272) { \n            // hover ë¡œ í•˜ë ¤ê³  í–ˆëŠ”ë°, $el ì´ target ì´ë¼ $dropzone:hover ì¸ì‹ì•ˆë¨.\n            // ì•ˆì— ë“¤ì–´ì˜¤ë©´ ë„“í˜€ì£¼ê³ , ë°°ê²½ìƒ‰ë¡œ ì•Œë ¤ì£¼ê¸°\n            $dropzone.style.width = '272px'\n            $dropzone.style.background = 'rgb(161, 161, 161, 0.2)';\n          }else {\n            // ê°ì§€ëœê±° ë‹¤ì‹œ reset í•´ì¤˜ì•¼í•¨.\n            $dropzone.style.width = '0px'\n          }\n        })\n      }\n\n      //! task ê°€ ì›€ì§ì¼ ë•ŒëŠ” ì„¸ë¡œ, ê°€ë¡œ ëª¨ë‘ ê³ ë ¤\n      else if($el.classList.contains('task')){\n        // $dropzone ìœ„ì¹˜ ê°ì§€\n        const $dropzones = document.querySelectorAll('.task-dropzone');\n        $dropzones.forEach($dropzone=>{\n          const x = $dropzone.getBoundingClientRect().x;\n          const y = $dropzone.getBoundingClientRect().y;\n\n          // ê°€ë¡œ ì„¸ë¡œ dropzone ë‚´ë¶€ ì²´í¬\n          if( (mouseX > x && mouseX < x + 272) && (mouseY > y && mouseY < y + 100 ) ) { \n            $dropzone.style.width = '256px';\n            $dropzone.style.height = '100px';\n            $dropzone.style.background = 'rgb(161, 161, 161, 0.2)';\n          }else {\n            // ê°ì§€ëœê±° ë‹¤ì‹œ reset í•´ì¤˜ì•¼í•¨.\n            $dropzone.style.height = '0px'\n          }\n        })\n      }\n    }\n  },\n  handleMouseUp : function(e, store, ids){ \n    // ì†ì„ ë†“ì•˜ì„ ë•Œ > new priority : state ë³€ê²½\n    // ids : ì¶œë°œì§€ ì •ë³´\n    //TODO dropzoneì€ className ì—ì„œ ì¶”ì¶œí•˜ëŠ” ìˆ˜ ë°–ì— ì—†ê² ì§€? ìš°ì„ ì€?\n    const { state: board, setState: setBoard } = store.board;\n    const { state: progresses, setState: setProgresses } = store.progresses;\n    const { state: tasks, setState: setTasks } = store.tasks;\n    const { changePrgPriority, changeTaskPriority }  = eventHandler;\n    const prev_priority = board.prg_priority;\n\n    const $el = document.querySelector(\".hold\");\n    if (!$el) return;\n\n    // progress, task ì¼ë°˜í™”\n    // prv_priorityëŠ” progress ë³€í™”ë¥¼ ìœ„í•´ì„œì„.\n\n    if( $el ){\n      // ì›€ì§ì´ë©´ ì ìš©ëœ ì†ì„± ë° classë¥¼ ì‚­ì œ\n      $el.removeAttribute(\"gap-x\")\n      $el.removeAttribute(\"gap-y\")\n      $el.classList.remove(\"hold\");\n\n      // ë†“ì—¬ì§„ ë§ˆìš°ìŠ¤ ì»¤ì„œì˜ XYì¢Œí‘œ\n      const mouseX = e.clientX;\n      const mouseY = e.clientY;\n\n\n      if(Object.keys(ids).length ===2){ //$el.classList.contains('progress')){\n\n        // ë†“ì—¬ì§„ progressId\n        const progressId = ids.progress_id;\n\n        // ë†“ì—¬ì§„ $dropzone ì°¾ê³ , ìƒˆë¡œìš´ priority ë¡œ state ë³€ê²½\n        const $dropzones = document.querySelectorAll('.prg-dropzone');\n        const split_prev_priority = prev_priority.split(',');\n\n        // progressId ì§€ì›€\n        const other_priority = split_prev_priority.filter(pri=>{\n          if(Number(pri) === progressId) return false;\n          else return true;\n        })\n\n        const new_priority = [];\n        for(let idx=0; idx<$dropzones.length; idx ++){\n          const x = $dropzones[idx].getBoundingClientRect().x;\n          if( mouseX > x && mouseX < x + 272) { \n            new_priority.push(progressId);\n          }else if(other_priority.length !== 0){\n            new_priority.push(other_priority.shift())\n          }\n        }\n\n        // state ë³€ê²½\n        if(new_priority.length === split_prev_priority.length \n          && new_priority.join(',') !== prev_priority){\n            changePrgPriority(store, new_priority.join(','), ids)\n        }\n\n        // dropzone í¬ê¸° ë°”ê¾¸ê¸° : ë§ˆì§€ë§‰ì´ë‚˜ ì²˜ìŒ $dropzone reset ì„ ìœ„í•´\n        $dropzones.forEach($dropzone=>{\n          $dropzone.style.width='0px'\n        })\n\n      } /* [ë] $el.classList.contains('progress') */\n      else if(Object.keys(ids).length === 3) { // target ì´ë™\n        e.stopPropagation();\n        let taskDropZone_Id = Array.from($el.classList).filter((str, idx)=>{\n          if(!str.match(/^taskDropZone-/)) return false;\n          else return true;\n        }) [0]\n\n        //! ì¶œë°œì§€ \n        const source = { //ì¶œë°œì§€ ì •ë³´\n          prgId: String(ids.progress_id), //progressId_taskId.split('-')[1],\n          taskId: String(ids.task_id), //progressId_taskId.split('-')[3],\n          taskDropZone: taskDropZone_Id.split('-')[1]\n        }\n\n        // ë†“ì—¬ì§„ $dropzone ì°¾ê³ , ìƒˆë¡œìš´ priority ë¡œ state ë³€ê²½\n        const $dropzones = document.querySelectorAll('.task-dropzone');\n\n        $dropzones.forEach($dropzone=>{\n          const x = $dropzone.getBoundingClientRect().x;\n          const y = $dropzone.getBoundingClientRect().y;\n\n          // ê°€ë¡œ ì„¸ë¡œ dropzone ë‚´ë¶€ ì²´í¬\n          if( (mouseX > x && mouseX < x + 272) && (mouseY > y && mouseY < y + 100 ) ) { \n            //! ë„ì°©ì§€\n            let progressId_taskId = Array.from($dropzone.classList).filter(str=>{\n              if(!str.match(/^prg-/)) return false;\n              else return true;\n            })[0];\n\n            const target = { //ë„ì°©ì§€ ì •ë³´\n              prgId: progressId_taskId.split('-')[1],\n              taskDropZone: progressId_taskId.split('-')[3]\n            }\n\n            //! ì¶œë°œì§€ ë„ì°©ì§€ ê°™ìœ¼ë©´ pass\n            if(source.prgId === target.prgId\n               && source.taskDropZone === target.taskDropZone){ // ê°™ì€ progressì— ë‘ì—ˆê³ \n              console.log(\"ì¶œë°œì§€ ë„ì°©ì§€ ê°™ìœ¼ë©´ pass\")\n            }else {\n              changeTaskPriority(store, ids, {source, target})\n            }\n            // statte ë³€ê²½\n\n          }else {\n            // ê°ì§€ëœê±° ë‹¤ì‹œ reset í•´ì¤˜ì•¼í•¨.\n            // $dropzone.style.height = '0px'\n          }\n        })\n\n        // dropzone í¬ê¸° ë°”ê¾¸ê¸° : ë§ˆì§€ë§‰ì´ë‚˜ ì²˜ìŒ $dropzone reset ì„ ìœ„í•´\n        $dropzones.forEach($dropzone=>{\n          $dropzone.style.height='0px'\n        })\n\n      } \n    }\n  }\n}","/home/ddubbu/git/ETON/1. ETON-client/src/helper/sortObject.js",[],"/home/ddubbu/git/ETON/1. ETON-client/src/components/board/TaskList.js",["151","152","153","154","155","156"],"/home/ddubbu/git/ETON/1. ETON-client/src/helper/eventHandler.js",["157","158","159","160","161","162","163","164","165","166","167","168","169","170","171","172","173","174","175","176","177","178","179","180","181"],"/home/ddubbu/git/ETON/1. ETON-client/src/components/modal/MemberDropDown.js",["182"],"import React, { useEffect } from 'react';\n\nexport default function MemberDorpDown ({members}) {\n\treturn (\n\t\t<ul className=\"drop-down member\">\n      {members.map((member, idx)=>{\n        return (\n        <li className={`member-${member.id}`} key={idx}>\n          {member.name}\n          <button>x</butt